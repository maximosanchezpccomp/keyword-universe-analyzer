# API Keys

# Anthropic (Claude) - Para análisis profundo y estratégico
# Obtén tu API key en: https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-api03-xxxxxxxxxxxxx

# OpenAI (GPT) - Para análisis rápido y económico
# Obtén tu API key en: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-xxxxxxxxxxxxx

# Semrush - Para obtener keywords directamente (opcional)
# Obtén tu API key en: https://www.semrush.com/api-analytics/
SEMRUSH_API_KEY=xxxxxxxxxxxxx

# Configuración de la aplicación
APP_TITLE="Keyword Universe Analyzer"
DEFAULT_MODEL="claude-sonnet-4-5-20250929"
MAX_KEYWORDS_PER_FILE=1000

# Configuración de análisis
DEFAULT_TIERS=3
MIN_VOLUME=10
DEFAULT_DATABASE=us

# Configuración de caché (opcional)
ENABLE_CACHE=true
CACHE_TTL=3600
